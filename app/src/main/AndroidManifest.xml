<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.bimbel">

    <!--
         The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but you must specify either coarse or fine
         location permissions for the 'MyLocation' functionality. 
    -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name="id.bimbel.MenuActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="id.bimbel.MapsActivity" />
        <activity android:name="id.bimbel.DetailBimbelActivity" />
        <activity android:name="id.bimbel.MainActivity"></activity>
        <activity android:name="id.bimbel.MapsAllBimbelActivity"></activity>
        <activity android:name="id.bimbel.DetailFromAllActivity"></activity>
        <activity android:name="id.bimbel.CariBimbelActivity"></activity>
        <activity android:name="id.bimbel.TerdekatActivity"></activity>
        <activity android:name="id.bimbel.MapsDetailActivity"></activity>

    </application>

</manifest>